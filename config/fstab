# Create mount point
cd /mnt
sudo mkdir nas
cd /mnt/nas
sudo mkdir AAA BBB CCC DDD EEE FFF GGG HHH III JJJ KKK

# NFS systemd (unRAID)

IP:/mnt/user/AAA	/mnt/nas/AAA	nfs	_netdev,nofail,x-systemd.automount,x-systemd.mount-timeout=10	0	0
IP:/mnt/user/BBB	/mnt/nas/BBB	nfs	_netdev,nofail,x-systemd.automount,x-systemd.mount-timeout=10	0	0
