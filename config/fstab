# Create mount point
sudo mkdir /media
cd /media
sudo mkdir AAA BBB CCC DDD EEE FFF GGG HHH III JJJ KKK

# NFS systemd (unRAID)

IP:/mnt/user/AAA  /media/AAA  nfs  _netdev,noauto,x-systemd.automount,x-systemd.mount-timeout=10  0 0
IP:/mnt/user/BBB  /media/BBB  nfs  _netdev,noauto,x-systemd.automount,x-systemd.mount-timeout=10  0 0
